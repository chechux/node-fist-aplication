<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include("partials/head")  %> 
  </head>
  <body>
    <%- include("partials/navigation") %> 

    <div class="container vh-100 d-flex justify-content-center align-items-center">
      <div class="card bg-dark text-center w-30">
        <div class="card-header">
          <h3>Contacto</h3>
        </div>
          <div class="card-body text-start">
            <form action="">
              
              <div class="mb-3">
                <label for="email">Email</label>
                <input class="form-control" type="text" placeholder="Emial..."/>
              </div>

              <div class="mb-3">
                <label for="message">Message:</label>
                <textarea class="form-control" type="text" rows="3" placeholder="Add message here..."></textarea>
              </div>

              <button class="btn btn-primary col-12">Send</button>
            </form>
          </div>
      </div>
    </div>
    <%- include("partials/footer")  %> 
  </body>
</html>
